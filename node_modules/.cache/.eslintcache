(theme) => ({\n  root: {\n    flexGrow: 1,\n  }\n}));\n\nfunction ApodSelection({ apodSelectionArray, onAddToProfileBtnClick, onDisselectBtnClickInSelection, currentProfile }) {\n  \n  //Material UI\n  const classes = useStyles();\n\n  //props to pass down for css purposes\n  const xsNum = 6;\n  const smNum = 3;\n  const seletedMemberCard = \"seletedMemberCard\";\n\n  //useHistory for the AddToTeamBtn\n  const history = useHistory();\n\n  const handleAddToProfileBtn = () => {\n    onAddToProfileBtnClick(apodSelectionArray)\n    // history.push(\"/team\")\n  }\n\n    return (\n        <div className=\"flex-container\">\n          <h1>{currentProfile}</h1>\n          <br></br>\n            <div className={classes.root}>\n              <Grid container spacing={3}>\n                {apodSelectionArray.map(apod => <ApodCard \n                                                    key={apod.date} \n                                                    apod={apod} \n                                                    apodTitle = {apod.title}\n                                                    apodImage={apod.url}\n                                                    apodId={apod.date}\n                                                    // link={`/recruit/${hero.id}`}\n                                                    linke={\"\"}\n                                                    hoverEffectDisabledId={\"hoverEffectDisabledId\"}\n                                                    xsNum={xsNum}\n                                                    smNum={smNum}\n                                                    seletedMemberCard={seletedMemberCard}\n                                                    onDisselectBtnClick={onDisselectBtnClickInSelection}\n                                                    />)}\n              </Grid>\n            </div>    \n            <button className=\"add-to-team-btn\" onClick={()=>{handleAddToProfileBtn()}}>Save Current Selection To Favorites</button>\n        </div>\n  );\n}\n\nexport default ApodSelection;\n","/Users/alexcalvert/Development/code/Shopify/front-end-application/spacestagram/src/components/NasaList.js",[],"/Users/alexcalvert/Development/code/Shopify/front-end-application/spacestagram/src/components/ApodCard.js",["71"],"import React from \"react\";\n\n// react-router-dom Imports\nimport { Link } from 'react-router-dom';\n\n//import css from material ui\nimport Grid from '@material-ui/core/Grid';\n\nfunction ApodCard( {apod, xsNum, smNum, seletedMemberCard, disselectBtnId, onDisselectBtnClick, apodImage, apodId, link, hoverEffectDisabledId} ) {\n\n  const handleDisselectBtn = () => {\n    onDisselectBtnClick(apod)\n  }\n\n  return (\n \n      <Grid item xs={xsNum} sm={smNum} >\n        <Link to={link} id={hoverEffectDisabledId}> \n          <div className=\"flex-card\" id={seletedMemberCard}>\n              <img className=\"img-in-card\" src={apodImage} />\n              <div className=\"hero-info-container\">\n                <h3 className=\"hero-name\">{apod.title}</h3>\n              </div>\n          </div>\n        </Link>\n        <button className=\"del-btn\" id={disselectBtnId} onClick={()=>{handleDisselectBtn()}}> Deselect</button>\n      </Grid>\n  );\n}\n\nexport default ApodCard;\n","/Users/alexcalvert/Development/code/Shopify/front-end-application/spacestagram/src/components/NasaDetails.js",["72","73"],"import React, { useEffect, useState } from \"react\";\nimport { useHistory, useParams } from 'react-router-dom'\nimport { makeStyles } from '@material-ui/core/styles';\nimport Popover from '@material-ui/core/Popover';\nimport Typography from '@material-ui/core/Typography';\n\nconst useStyles = makeStyles((theme) => ({\n  typography: {\n    padding: theme.spacing(2),\n    color: \"black\"\n  },\n}));\n\nfunction NasaDetails({ onApodSelection, isLoggedIn, setApodArray, apodArray, apiKey }) {\n    const [apod, setApod] = useState(null)\n    const [isLoaded, setIsLoaded] = u